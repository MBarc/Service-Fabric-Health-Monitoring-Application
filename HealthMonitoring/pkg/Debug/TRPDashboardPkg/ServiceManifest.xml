<?xml version="1.0" encoding="utf-8"?>
<ServiceManifest Name="TRPDashboardPkg"
                 Version="1.0.30"
                 xmlns="http://schemas.microsoft.com/2011/01/fabric"
                 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ServiceTypes>
		<!-- This is the name of your ServiceType. -->
		<StatelessServiceType ServiceTypeName="TRPDashboardType" />
	</ServiceTypes>

	<!-- Code package is your service executable. -->
	<CodePackage Name="Code" Version="1.0.30">
		<EntryPoint>
			<ExeHost>
				<Program>TRPDashboard.exe</Program>
				<WorkingFolder>CodePackage</WorkingFolder>
			</ExeHost>
		</EntryPoint>
	</CodePackage>

	<!-- Config package removed since we're not using Settings.xml -->

	<Resources>
		<Endpoints>
			<!-- This endpoint is used by the communication listener to obtain the port on which to 
           listen. Please note that if your service is partitioned, this port is shared with 
           replicas of different partitions that are placed in your code. -->
			<Endpoint Name="ServiceEndpoint" Type="Input" Protocol="http" Port="8081" />
		</Endpoints>
	</Resources>
</ServiceManifest>